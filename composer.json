{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "anandsiddharth/laravel-paytm-wallet": "^2.0",
        "brian2694/laravel-toastr": "^5.54",
        "doctrine/dbal": "^3.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "hisorange/browser-detect": "^4.3",
        "intervention/image": "^2.5",
        "jorenvanhocht/laravel-share": "^4.0",
        "kingflamez/laravelrave": "^4.2",
        "laravel/framework": "^8.12",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.1",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.1",
        "laravelcollective/html": "^6.2",
        "league/omnipay": "^3.1",
        "livewire/livewire": "^2.1",
        "maatwebsite/excel": "^3.1",
        "mews/purifier": "^3.3",
        "midtrans/midtrans-php": "^2.3",
        "niklasravnsborg/laravel-pdf": "^4.0",
        "nwidart/laravel-modules": "^8.2",
        "omnipay/paypal": "^3.0",
        "ourcodeworld/name-that-color": "9999999-dev",
        "razorpay/razorpay": "^2.5",
        "silviolleite/laravelpwa": "^2.0",
        "softon/indipay": "^1.2",
        "spatie/db-dumper": "^2.21",
        "spatie/laravel-analytics": "^3.11",
        "spondonit/amazcart-service": "^1.0",
        "stevebauman/location": "^6.1",
        "stripe/stripe-php": "^7.69",
        "twilio/sdk": "^6.15",
        "unicodeveloper/laravel-paystack": "^1.0",
        "van-ons/laraberg": "^1.1",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "google/apiclient-services": [
            "GoogleAnalyticsAdmin"
        ]
    },
    "autoload": {
        "files": [
            "app/Helpers/Common.php",
            "app/Helpers/Functions.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Modules\\": "Modules/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
